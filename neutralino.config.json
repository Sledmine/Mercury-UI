{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/mercury-ui-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "nativeAllowList": [
    "app.*",
    "os.execCommand",
    "os.spawnProcess",
    "os.updateSpawnedProcess",
    "os.getEnv",
    "os.showFolderDialog",
    "os.showOpenDialog",
    "os.getPath"
  ],
  "modes": {
    "window": {
      "title": "Mercury UI",
      "width": 900,
      "height": 510,
      "minWidth": 800,
      "minHeight": 510,
      "icon": "/mercury-ui-react/public/mercury-logo.png",
      "useSavedState": true
    }
  },
  "cli": {
    "binaryName": "mercury-ui",
    "resourcesPath": "/mercury-ui-react/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/mercury-ui-react/public/neutralino.js",
    "binaryVersion": "4.15.0",
    "clientVersion": "3.13.0",
    "frontendLibrary": {
      "patchFile": "/mercury-ui-react/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  },
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  }
}