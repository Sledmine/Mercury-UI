{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/mercury-ui-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.execCommand",
    "os.spawnProcess",
    "os.updateSpawnedProcess"
  ],
  "modes": {
    "window": {
      "title": "Mercury UI",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/mercury-ui-react/public/mercury-logo.png"
    }
  },
  "cli": {
    "binaryName": "mercury-ui",
    "resourcesPath": "/mercury-ui-react/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/mercury-ui-react/public/neutralino.js",
    "binaryVersion": "4.11.0",
    "clientVersion": "3.8.2",
    "frontendLibrary" : {
      "patchFile": "/mercury-ui-react/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}